{
  "name": "hello-world-react",
  "version": "1.4.0-dev",
  "private": true,
  "mobify": {
    "ssrEnabled": true,
    "ssrOnly": [
      "ssr.js",
      "ssr.js.map",
      "node_modules/**/*.*"
    ],
    "ssrShared": [
      "**/*.js",
      "**/*.js.map",
      "**/*.json"
    ],
    "ssrParameters": {
      "ssrFunctionNodeVersion": "14.x",
      "proxyConfigs": []
    }
  },
  "scripts": {
    "build": "sdk-pwa-build",
    "push": "npm run build && sdk-upload",
    "save-credentials": "sdk-save-credentials",
    "start": "node build/ssr.js"
  },
  "license": "See license in LICENSE",
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "apollo-boost": "^0.4.9",
    "core-js": "^3.20.2",
    "cross-fetch": "^3.1.4",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "prop-types": "^15.8.1",
    "pwa-kit-react-sdk": "^1.4.0-dev",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "svg-sprite-loader": "^6.0.11",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
